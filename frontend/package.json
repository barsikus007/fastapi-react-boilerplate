{
    "name": "fastapi-react-boilerplate",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "lint": "eslint --ext ts,tsx --cache --report-unused-disable-directives --max-warnings 0",
        "lint:all": "pnpm lint src",
        "lint:fix": "eslint --ext ts,tsx --cache --fix",
        "lint:fix-all": "pnpm lint:fix src",
        "format-lint": "pnpm prettier-eslint",
        "format-lint:fix": "pnpm format-lint --write",
        "format-lint:fix-all": "pnpm format-lint:fix src/**/*.ts{x,}",
        "preview": "vite preview"
    },
    "engineStrict": true,
    "engines": {
        "node": "^20"
    },
    "dependencies": {
        "@emotion/react": "^11.11.3",
        "@emotion/styled": "^11.11.0",
        "dayjs": "^1.11.10",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-query": "^3.39.3",
        "react-router-dom": "^6.22.0"
    },
    "devDependencies": {
        "@nabla/vite-plugin-eslint": "^2.0.2",
        "@types/react": "^18.2.55",
        "@types/react-dom": "^18.2.19",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "@vitejs/plugin-react-swc": "^3.6.0",
        "eslint": "^8.56.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "prettier": "^3.2.5",
        "prettier-eslint": "^16.3.0",
        "prettier-eslint-cli": "^8.0.1",
        "typescript": "^5.3.3",
        "vite": "^5.0.12",
        "vite-tsconfig-paths": "^4.3.1"
    }
}
